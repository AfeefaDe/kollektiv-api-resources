{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@afeefa/api-resources-client/*": [
        "../../api-resources-client/src/*"
      ],
      "@a-vue/*": [
        "node_modules/@afeefa/vue-app/src/*"
      ],
      "@a-admin/*": [
        "node_modules/@afeefa/vue-app/src-admin/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "../../api-resources-client/src/*",
    "node_modules/@afeefa/vue-app/src/**/*",
    "node_modules/@afeefa/vue-app/src-admin/**/*"
  ]
}
